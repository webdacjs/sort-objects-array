{
  "name": "sort-objects-array",
  "version": "3.0.5",
  "description": "Simple module to sort an array of objects by a property without mutating the original array.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts",
    "dev": "tsup --watch",
    "test": "vitest",
    "prepare": "npm run build"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "legacyOutput": true,
    "splitting": false,
    "format": [
      "cjs",
      "esm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webdacjs/sort-objects-array.git"
  },
  "keywords": [
    "sort",
    "array",
    "object",
    "order"
  ],
  "author": "Juan Convers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdacjs/sort-objects-array/issues"
  },
  "homepage": "https://github.com/webdacjs/sort-objects-array#readme",
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
